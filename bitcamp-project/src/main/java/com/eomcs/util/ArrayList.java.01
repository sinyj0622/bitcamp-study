package com.eomcs.util;

public class ArrayList {
  Object[] elementData;
  int size; // 몇 개 준비되는지 항상 필요함, 인스턴스필드 초기값은 0이다

  public ArrayList() { // 컴파일러가 new Object[100]을 생성자로 복사해서 읽음
    super();

    this.elementData = new Object[100]; 
  }

  public ArrayList(int initialCapacity) {
    super();

    this.elementData = new Object[initialCapacity]; //생성자에 넣으면 초기값을 다르게 설정가능~ 선택권을 여러게
  }


}
